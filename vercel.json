{
  "version": 1,
  "builds": [
    {
      "src": "*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "headers": {
        "Access-Control-Allow-Origin": "https://creativedevops.com",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization",
        "Access-Control-Allow-Credentials": "true"
      },
      "dest": "/"
    }
  ],
  "functions": {
    "api/test.js": {
      "maxDuration": 30 
    },
    "api/*.js": {
      "maxDuration": 15 
    },
    "src/api/*.js": {
      "maxDuration": 25 
    }
  }
}
